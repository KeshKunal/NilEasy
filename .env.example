# filepath: d:\Projects\NilEasy\.env.example
# Application
APP_NAME=NilEasy
ENVIRONMENT=development
DEBUG=true

# MongoDB Atlas
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority
MONGODB_DB_NAME=nileasy_db

# Session Settings
SESSION_TIMEOUT_MINUTES=60
MAX_MESSAGES_PER_DAY=100

# GST Service
GST_SERVICE_URL=http://localhost:5001
GST_SERVICE_TIMEOUT=30

# Rate Limiting
RATE_LIMIT_MESSAGES_PER_MINUTE=5
RATE_LIMIT_GSTIN_LOOKUPS_PER_HOUR=10

# WhatsApp/Twilio
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# AiSensy
AISENSY_API_KEY=your_key_here
AISENSY_WEBHOOK_SECRET=your_secret_here

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log