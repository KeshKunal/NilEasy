# Environment Variables Example
# Copy this file to .env and fill in your actual values

# MongoDB
MONGODB_URI=mongodb://localhost:27017
DATABASE_NAME=nileasy

# WhatsApp/AiSensy
AISENSY_API_KEY=your_api_key_here
AISENSY_WEBHOOK_SECRET=your_webhook_secret

# GST API
GST_API_URL=https://gst.api.example.com
GST_API_KEY=your_gst_api_key

# SMS Service
SMS_PROVIDER_API_KEY=your_sms_api_key

# Application
DEBUG=False
LOG_LEVEL=INFO
